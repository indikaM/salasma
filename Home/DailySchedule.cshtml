@{
     Layout = "~/Shared/Layouts/_MainLayout.cshtml";
     Page.Title = "Daily Schedule";  


     
          string[] hourlyPlan = {"00:00","01:00","02:00","03:00","04:00","05:00","06:00","07:00","08:00","09:00","10:00","11:00","12:00","13:00",
                            "14:00","15:00","16:00","17:00","18:00","19:00","20:00","21:00","22:00","23:00"};
}

<div class="scheduleDailyPlanPane">
    <div class="scheduleDailyTimeSheetPane">
     <fieldset  class="widgetWindow">
       <div class="widgetHeader">&nbsp;&nbsp;Schedule </div>
         <div class="scheduleDailyTaskTime">
                <div class="widgetTableDiv">
                    <table class="widgetTable">
                        <thead>
                           <tr>
                             <th>Time</th>
                             <th width="80%">Task</th>
                           </tr>
                        </thead>
                        <tbody>
                           @foreach(var plan in hourlyPlan){
                                <tr>
                                    <td>@plan</td>
                                    <td>plan.duration</td>
                                </tr> 
                            }
                        </tbody>
                    </table>
            </div>
        </div>
        <div class="scheduleDailyTask">
            <div class="widgetTableDiv">
                    <table class="widgetTable">
                        <tbody>
                           @foreach(var plan in hourlyPlan){
                                <tr>
                                    <td>Task</td>
                                </tr> 
                            }
                        </tbody>
                    </table>
                </div>
            </div>
     </fieldset>
    </div>

</div>